<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	<managed-bean>
		<managed-bean-name>kundeMB</managed-bean-name>
		<managed-bean-class>bank.mb.KundeMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<!-- Hauptmenue -->
	<navigation-rule>
		<from-view-id>Hauptmenue.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>KUNDENVERWALTUNG_MENUE</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>BACK_TO_HAUPTMENUE</from-outcome>
			<to-view-id>/Hauptmenue.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>

	<!-- Kundenverwaltungsmenue -->
	<navigation-rule>
		<from-view-id>/pages/public/kundenvw/KundenVerwaltungMenue.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_HAUPTMENUE</from-outcome>
			<to-view-id>/Hauptmenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>KUNDEN_ANLEGEN</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundeAnlegen.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>KUNDEN_LISTANZEIGE</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundeListanzeige.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>SHOW_EINGABE_SUCHPARAMETER</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundeSuchen.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>

	<!-- Eingabe neuer Kunde -->
	<navigation-rule>
		<from-view-id>/pages/public/kundenvw/KundeAnlegen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>		
	</navigation-rule>
	
	<!--  Eingabe Suchparameter -->
	<navigation-rule>
		<from-view-id>/pages/public/kundenvw/KundeSuchen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>SHOW_SUCH_ERGEBNIS</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundensucheErgebnisliste.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Kunden Suchergebnis anzeigen -->
	<navigation-rule>
		<from-view-id>/pages/public/kundenvw/KundensucheErgebnisliste.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		<from-outcome>UPDATE_PRIVATKUNDE</from-outcome>
			<to-view-id>/pages/public/kundenvw/PrivatkundeUpdaten.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-outcome>UPDATE_GESCHAEFTSKUNDE</from-outcome>
			<to-view-id>/pages/public/kundenvw/GeschaeftskundeUpdaten.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-outcome>DELETE_KUNDE</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundeLoeschen.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>
	
	<!-- Anzeige aller Kunden -->
	<navigation-rule>
		<from-view-id>/pages/public/kundenvw/KundeListanzeige.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>		
		<navigation-case>
			<from-outcome>UPDATE_PRIVATKUNDE</from-outcome>
			<to-view-id>/pages/public/kundenvw/PrivatkundeUpdaten.xhtml</to-view-id>
		</navigation-case>	
				<navigation-case>
			<from-outcome>UPDATE_GESCHAEFTSKUNDE</from-outcome>
			<to-view-id>/pages/public/kundenvw/GeschaeftskundeUpdaten.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-outcome>DELETE_KUNDE</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundeLoeschen.xhtml</to-view-id>
		</navigation-case>	
		
	</navigation-rule>
	
	<!--  Update Geschaeftskunde -->
	<navigation-rule>
		<from-view-id>/pages/public/kundenvw/GeschaeftskundeUpdaten.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENLISTANZEIGE</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundeListanzeige.xhtml</to-view-id>
		</navigation-case>		
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!--  Update Privatkunden -->
	<navigation-rule>
		<from-view-id>/pages/public/kundenvw/PrivatkundeUpdaten.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENLISTANZEIGE</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundeListanzeige.xhtml</to-view-id>
		</navigation-case>		
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Kunde Loeschen -->
	<navigation-rule>
		<from-view-id>/pages/public/kundenvw/KundeLoeschen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENLISTANZEIGE</from-outcome>
			<to-view-id>/pages/public/kundenvw/KundeListanzeige.xhtml</to-view-id>
		</navigation-case>		
	</navigation-rule>
	
	
</faces-config>
