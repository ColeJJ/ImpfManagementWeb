			<to-view-id>/pages/protected/kundenvw/KundeListanzeige.xhtml</to-view-id>
		</navigation-case>		
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
