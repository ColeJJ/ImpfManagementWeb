	<!-- Kunde Loeschen -->
	<navigation-rule>
		<from-view-id>/pages/protected/kundenvw/KundeLoeschen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENLISTANZEIGE</from-outcome>
