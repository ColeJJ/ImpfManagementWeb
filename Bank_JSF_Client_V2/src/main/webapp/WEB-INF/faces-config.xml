<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	<managed-bean>
		<managed-bean-name>userMB</managed-bean-name>
		<managed-bean-class>bank.mb.UserMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>kundeMB</managed-bean-name>
		<managed-bean-class>bank.mb.KundeMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>kontoMB</managed-bean-name>
		<managed-bean-class>bank.mb.KontoMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>applicationSecurityConfig</managed-bean-name>
		<managed-bean-class>bank.mb.ApplicationSecurityConfig</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>menuMB</managed-bean-name>
		<managed-bean-class>bank.mb.MenuMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>	

	<!-- Hauptmenue -->
	<navigation-rule>
		<from-view-id>/pages/protected/Hauptmenue.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>KUNDENVERWALTUNG_MENUE</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>KONTENVERWALTUNG_MENUE</from-outcome>
			<to-view-id>/pages/protected/kontenvw/KontenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>BACK_TO_HAUPTMENUE</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

	<!-- Kundenverwaltungsmenue -->
	<navigation-rule>
		<from-view-id>/pages/protected/kundenvw/KundenVerwaltungMenue.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_HAUPTMENUE</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>KUNDEN_ANLEGEN</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundeAnlegen.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>KUNDEN_LISTANZEIGE</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundeListanzeige.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>SHOW_EINGABE_SUCHPARAMETER</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundeSuchen.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

	<!-- Eingabe neuer Kunde -->
	<navigation-rule>
		<from-view-id>/pages/protected/kundenvw/KundeAnlegen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
			<redirect/>
		</navigation-case>	
	</navigation-rule>
	
	<!--  Eingabe Suchparameter -->
	<navigation-rule>
		<from-view-id>/pages/protected/kundenvw/KundeSuchen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>SHOW_SUCH_ERGEBNIS</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundensucheErgebnisliste.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
			<redirect/>			
		</navigation-case>
	</navigation-rule>
	
	<!-- Kunden Suchergebnis anzeigen -->
	<navigation-rule>
		<from-view-id>/pages/protected/kundenvw/KundensucheErgebnisliste.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BACK_TO_KUNDENVW</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundenVerwaltungMenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		<from-outcome>UPDATE_PRIVATKUNDE</from-outcome>
			<to-view-id>/pages/protected/kundenvw/PrivatkundeUpdaten.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-outcome>UPDATE_GESCHAEFTSKUNDE</from-outcome>
			<to-view-id>/pages/protected/kundenvw/GeschaeftskundeUpdaten.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-outcome>DELETE_KUNDE</from-outcome>
			<to-view-id>/pages/protected/kundenvw/KundeLoeschen.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
