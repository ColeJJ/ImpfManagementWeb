		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
			<redirect/>
		</navigation-case>				
	</navigation-rule>
	
	<!-- Kontoverwaltung  -->
	<!--  -->
	<navigation-rule>
		<from-view-id>/pages/protected/kontenvw/KontenVerwaltungMenue.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>KONTO_ANLEGEN</from-outcome>
			<to-view-id>/pages/protected/kontenvw/KontoAnlegen.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
			<from-outcome>KONTOBEWEGUNG_BUCHEN</from-outcome>
			<to-view-id>/pages/protected/kontenvw/KontobewegungBuchen.xhtml</to-view-id>
		</navigation-case>			
		<navigation-case>
			<from-outcome>BACK_TO_HAUPTMENUE</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/pages/protected/Hauptmenue.xhtml</to-view-id>
